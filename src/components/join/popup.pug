#popup-form 
	.popup-title Join Ching Luh
	.wrap-form 
		.form-row 
			.form-group 
				input(type="text" placeholder="Full name")
			.form-group 
				input(type="text" placeholder="Phone number")
		.form-row 
			.form-group 
				input(type="text" placeholder="Email")
			.form-group 
				input(type="text" placeholder="Title")
		.form-group 
			textarea(type="text" placeholder="Content")
		.form-group.qupload 
			label.label(for="") Attached files (rar, zip, doc, docx, pdf < 1Mb)
			.RadUpload
				ul.ruInputs
					li 
						span.ruFileWrap 
							input.ruButton(type="button" value="Select")
							input.ruFileInput(type="file")
		.formwizard-bottom
			.form-group.frm-btnwrap
				.frm-btn
					input.btn.btn-default.frm-btn-submit(type="submit" value="Send")
